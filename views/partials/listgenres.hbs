{{#each}}
  <section class="ui stacked segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <p> Total Duration: {{duration}} </p>
    <a href="/genre/{{id}}" class="ui inverted icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/library/deletegenre/{{id}}" class="ui inverted icon button">
      <i class="icon trash"></i>
    </a>
  </section>
{{/each}}